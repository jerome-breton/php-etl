#!/usr/bin/env php
<?php
// application.php

require __DIR__.'/../vendor/autoload.php';

use EtlBundle\Console\ListCommand;
use EtlBundle\Console\RunCommand;
use Symfony\Component\Console\Application;

$application = new Application('PHP ETL');
$application->add(new RunCommand());
$application->add(new ListCommand());

$application->run();